CREATE TABLE `COLLORG`.`customers` (
`id` SMALLINT NOT NULL AUTO_INCREMENT ,
`account` SMALLINT NOT NULL ,
`last` VARCHAR( 32 ) NOT NULL ,
`first` VARCHAR( 32 ) NOT NULL ,
`isInactive` TINYINT NULL ,
PRIMARY KEY ( `id` )
) ENGINE = MYISAM ;

INSERT INTO `COLLORG`.`customers` (
