http://localhost/MAMP/

MySQL

The MySQL Database can be administrated with phpMyAdmin.

To connect to the MySQL Server from your own scripts use the following connection parameters:
Host 	localhost
Port 	3306
User 	root
Password 	root
Example:

$link = mysql_connect('localhost', 'root', 'root');

or you can connect using an UNIX Socket:
Socket 	/Applications/MAMP/tmp/mysql/mysql.sock
User 	root
Password 	root

Example:

$link = mysql_connect(
  ':/Applications/MAMP/tmp/mysql/mysql.sock',
  'root',
  'root'
);


    * Server: Localhost via UNIX socket
    * Server version: 5.1.37
    * Protocol version: 10
    * User: root@localhost
    * MySQL charset: UTF-8 Unicode (utf8)

        * Apache/2.0.63 (Unix) PHP/5.2.11 DAV/2
    * MySQL client version: 5.1.37
    * PHP extension: mysql

    Version information: 3.2.0.1
    
    create COLLORG
    
 CREATE TABLE `COLLORG`.`options` (
`id` SMALLINT NOT NULL AUTO_INCREMENT ,
`account` SMALLINT NOT NULL ,
`name` VARCHAR( 32 ) NOT NULL ,                                        
`description` VARCHAR( 256 ) NOT NULL ,
`isInactive` TINYINT NULL ,        
PRIMARY KEY ( `id` )
) ENGINE = MYISAM ;
                               
INSERT INTO `COLLORG`.`options` (`id`, `account`, `name`, `description`, `isInactive`) VALUES ('1', '0', 'basic', 'basic', NULL), ('2', '0', 'standard', 'standard', NULL), ('3', '0', 'premium', 'premium', NULL);

//ALTER TABLE `options` ADD `isInactive` TINYINT NULL 

ALTER TABLE `options` CHANGE `description` `description` VARCHAR( 256 ) CHARACTER SET latin1 COLLATE latin1_swedish_ci NULL; 

//ALTER TABLE `options` CHANGE `id` `id` SMALLINT( 6 ) NOT NULL AUTO_INCREMENT 

CREATE TABLE `COLLORG`.`types` (
`id` SMALLINT NOT NULL AUTO_INCREMENT ,
`account` SMALLINT NOT NULL ,
`name` VARCHAR( 32 ) NOT NULL ,
`description` VARCHAR( 256 ) NOT NULL ,
`isInactive` TINYINT NULL ,
PRIMARY KEY ( `id` )
) ENGINE = MYISAM ;

//ALTER TABLE `types` DROP INDEX `id` 

