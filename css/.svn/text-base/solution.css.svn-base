.ui-dialog-titlebar-close { float: right; padding: 1px; height: 18px; margin: .1em .5em .2em .1em; }

#scrumDialog hr {margin: 0; padding: 0;}

/*.ui-dialog-content { background-color:#a1d2f6; }*/
.ui-dialog-content { clear: both; margin: 1em 1em 1em 2em; background-color:#cccccc; }

.plot {margin:0 0 0 20px;}

p.scrumNoteOK {color: #006600; font-size: 12px; font-weight: bold;}
p.scrumNoteCaution {color: #CC3300; font-size: 12px; font-weight: bold;}
p.scrumNoteAlert {color: #FF0000; font-size: 12px; font-weight: bold;}
